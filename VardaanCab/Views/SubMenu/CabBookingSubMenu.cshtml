@model VardaanCab.Domain.ViewModels.CabBookingSubMenuModel
@{
    ViewBag.Title = "CabBookingSubMenu";
    Layout = null;


}

<link href="/css/component.css" rel="stylesheet" />
<link href="/css/style_grid.css" rel="stylesheet" />
<link href="/css/font-awesome.css" rel="stylesheet">
<!DOCTYPE html>
<head>
    <style>
         .containerDiv {
  width:100%;
  /*height:650px;
  margin: auto;
  border:1px solid;*/
}
        .btnBack {
 background-color: #008CBA;/* Blue */
  border: none;
  color: white;
  padding: 15px 32px;
  text-align: center;
  text-decoration: none;
  display: inline-block;
  font-size: 16px;
  margin: 4px 2px;
  cursor: pointer;
  width:10%;
}
    </style>
</head>
<body>

 <div class="containerDiv">
            <a href="/Admin/Dashboard" class="btnBack">Back</a>

            <div class="agile_top_w3_grids">
                <ul class="ca-menu">
                    
                    <li>
                        <a href="/Booking/CabBook">
                            <i class="fa fa-list" aria-hidden="true"></i>
                            <div class="ca-content">
                                @*<h4 class="ca-main one">Allot Vehicle</h4>*@
                                <h3 class="ca-sub one">Create Booking</h3>
                            </div>
                        </a>
                    </li>

                    <li>
                        <a href="/MonthlyPackage/All">
                            <i class="fa fa-list" aria-hidden="true"></i>
                            <div class="ca-content">
                                <h3 class="ca-sub one">Monthly Booking (@Model.MonthlyBooking)</h3>
                            </div>
                        </a>
                    </li>

                    <li>
                        <a href="/MonthlyRoutePackage/All">
                            <i class="fa fa-list" aria-hidden="true"></i>
                            <div class="ca-content">
                                <h3 class="ca-sub one">Monthly Route Booking (@Model.MonthlyRouteBooking)</h3>
                            </div>
                        </a>
                    </li>
                    <li>
                        <a href="/Booking/ShowAllBooking">

                            <i class="fa fa-list" aria-hidden="true"></i>
                            <div class="ca-content">
                                @*<h4 class="ca-main">User's Complaints</h4>*@
                                <h3 class="ca-sub">All Bookings (@Model.AllBooking)</h3>
                            </div>
                        </a>
                    </li>
                             <li>
                        <a href="/Booking/ShowBooking">

                            <i class="fa fa-list" aria-hidden="true"></i>
                            <div class="ca-content">
                                @*<h4 class="ca-main">User's Complaints</h4>*@
                                <h3 class="ca-sub">Pending Bookings (@Model.PendingBooking)</h3>
                            </div>
                        </a>
                    </li>

                    <li>
                        <a href="/Booking/ShowBooking?bookingStatus=@((int)VardaanCab.Utilities.BookingStatus.Dispatch)">

                            <i class="fa fa-list" aria-hidden="true"></i>
                            <div class="ca-content">
                                @*<h4 class="ca-main">User's Complaints</h4>*@
                                <h3 class="ca-sub">Dispatched Bookings (@Model.DispatchedBooking)</h3>
                            </div>
                        </a>
                    </li>

                    <li>
                        <a href="/Booking/ShowBooking?bookingStatus=@((int)VardaanCab.Utilities.BookingStatus.Completed)">

                            <i class="fa fa-list" aria-hidden="true"></i>
                            <div class="ca-content">
                                @*<h4 class="ca-main">User's Complaints</h4>*@
                                <h3 class="ca-sub">Completed Bookings (@Model.CompletedBooking)</h3>
                            </div>
                        </a>
                    </li>

                    <li>
                        <a href="/Booking/ShowBooking?bookingStatus=@((int)VardaanCab.Utilities.BookingStatus.Cancelled)">

                            <i class="fa fa-list" aria-hidden="true"></i>
                            <div class="ca-content">
                                @*<h4 class="ca-main">User's Complaints</h4>*@
                                <h3 class="ca-sub">Cancelled Bookings (@Model.CancelledBooking)</h3>
                            </div>
                        </a>
                    </li>
                    
                  

                    <div class="clearfix"></div>
                </ul>
            </div>
        </div>

</body>






